{
  "schemaVersion": 1,
  "id": "crimeutils",
  "version": "${version}",
  "name": "CrimeUtils",
  "description": "General purpose mod used in Crimecraft S3 that includes any extra things that were custom-made.",
  "authors": [
    "BluSpring"
  ],
  "contact": {
    "website": "https://bluspring.xyz",
    "repo": "https://github.com/BluSpring/CrimeUtils"
  },
  "license": "MIT",
  "icon": "crimecraft.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "xyz.bluspring.crimeutils.CrimeUtils"
    ],
    "client": [
      "xyz.bluspring.crimeutils.client.CrimeUtilsClient"
    ],
    "cardinal-components": [
      "xyz.bluspring.crimeutils.worldgen.SpawnerComponents"
    ]
  },
  "mixins": [
    "crimeutils.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-language-kotlin": "*",
    "simplehats": ">=0.1.6",
    "trinkets": ">=3.4.1"
  },
  "custom": {
    "cardinal-components": [
      "crimecraft:last_spawn",
      "crimecraft:entity_count",
      "crimecraft:reset_at"
    ]
  }
}
